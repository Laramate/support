{
    "name": "laramate/support",
    "description": "Support Package for Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
