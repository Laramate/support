{
    "name": "laramate/support",
    "description": "Support Package for Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://github.com/tkivelip",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*",
        "illuminate/database": "5.6.*|5.7.*|5.8.*|6.*|7.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Laramate\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramate\\Support\\Tests\\": "tests/"
        }
    }
}
